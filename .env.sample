# 911 Dispatch Monitor - Environment Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================

# PostgreSQL server hostname or IP address
DB_HOST=localhost

# PostgreSQL port (default: 5432)
DB_PORT=5432

# Database name
DB_NAME=dispatch_911

# Database username
DB_USER=your_db_user

# Database password (REQUIRED - no default for security)
DB_PASSWORD=your_secure_password

# =============================================================================
# API CONFIGURATION (for Python ingester)
# =============================================================================

# FirstWatch API token (public token from the website)
DISPATCH_API_TOKEN=your_api_token

# =============================================================================
# SERVER CONFIGURATION (for Express API)
# =============================================================================

# API server port
API_PORT=3002

# Allowed CORS origins (comma-separated for multiple)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000
